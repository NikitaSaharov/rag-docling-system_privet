# Polza.ai API (для DeepSeek)
POLZA_API_KEY=your_polza_api_key_here

# OpenRouter API (deprecated, используйте Polza)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Telegram Bot
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# Qdrant
QDRANT_API_KEY=your_secure_random_key_here

# PostgreSQL
POSTGRES_DB=rag_logs
POSTGRES_USER=rag_user
POSTGRES_PASSWORD=your_secure_password_here
DATABASE_URL=postgresql://rag_user:your_secure_password_here@postgres:5432/rag_logs

# Redis
REDIS_PASSWORD=your_redis_password_here
REDIS_URL=redis://:your_redis_password_here@redis:6379/0

# Flask
SECRET_KEY=your_flask_secret_key_32_chars_minimum
FLASK_ENV=production

# Database (SQLite для пользователей)
DB_PATH=/db/docling.db

# Flask API URL (для Telegram бота)
FLASK_API_URL=http://webapp:5000

# Security
ALLOWED_HOSTS=yourdomain.com,www.yourdomain.com
CORS_ORIGINS=https://yourdomain.com

# Monitoring (optional)
SENTRY_DSN=your_sentry_dsn_here
